<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.may.mapper.CompetenceMapper">

    <select id="queryAll" resultType="com.may.pojo.Competence">
        select *
        from test01.competence
    </select>

    <select id="query" resultType="com.may.pojo.Competence">
        select * from test01.competence where id = #{id}
    </select>
</mapper>